{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
            "standard",
            "prettier"
    ],
    "parser": "babel-eslint",
    "plugins": [
            "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": "error",
        "no-console": "off"
    }
}
